{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "@mate-academy/eslint-config",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint"],
  "rules": {
    "react/no-unescape-entities": "off"
  }
}
