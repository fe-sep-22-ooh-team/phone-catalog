    "eslint": "^8.31.0",
    "eslint-plugin-react": "^7.31.11",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "2.8.1",
    "sass": "^1.57.1",
    "node-sass": "^7.0.1",
    "stylelint": "^13.13.1",
    "typescript": "^4.6.3"
